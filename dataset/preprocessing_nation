    "# '#' 제거\n",
    "keyward = []\n",
    "for c in range(len(key)) :\n",
    "        if key[c][0].endswith('분') == True :\n",
    "            if '관람가' in key[c][0] == True :\n",
    "                keyward.append(0)\n",
    "            keyward.append(key[c][1].strip('#').strip(' '))\n",
    "        else :\n",
    "            keyward.append(key[c][0].strip('#').strip(' '))\n",
    "keyward"
   ]
